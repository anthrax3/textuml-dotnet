@using TextUml.Extensions
<div id="profile-dialog" class="modal hide fade">
  <div class="modal-header">
    <a href="javascript:;" rel="nofollow" class="close" data-dismiss="modal" title="close">&times;</a>
    <h3>My account</h3>
  </div>
  <div class="modal-body">
    <div class="modal-section">
      @Html.ClientView("ChangePassword")
    </div>
    <div class="modal-section">
      @Html.ClientView("SignOut")
    </div>
  </div>
</div>